#include <iostream>
using namespace std;
// row = fila | col = columna
int main() {
    int c = 9; 
    for(int row = 1; row<=c; row++)
    {
        for(int space =1; space < c-row; space++)
            cout << " ";
        for(int col=1; col<=row*2-1;col++) 
        {
            cout << row;
            
        }
        cout<< endl;
    }
    return 0;
}
