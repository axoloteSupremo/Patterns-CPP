#include <iostream>
using namespace std;

int main() {
    for(int row = 1; row<=10; row++) 
    {
        for(int col=1; col <= 10; col++)
        {
            if (row == 1 || row == 10 || col == 1 || col == 10)
                cout << "# ";
            else 
                cout << "  ";
        }
        cout << endl;
            
            
    }
    
    return 0;
}
